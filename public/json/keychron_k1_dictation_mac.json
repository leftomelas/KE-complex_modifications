{
  "title": "Map Keychron Dictation Key on Mac",
  "maintainers": [
    "paritoshshah"
  ],
  "rules": [
    {
      "description": "Map Keychron Dictation Key on Mac",
      "manipulators": [
        {
          "from": {
            "simultaneous": [
              { "key_code": "spacebar" },
              { "key_code": "fn" }
            ]
          },
          "to": [{ "consumer_key_code": "dictation" }],
          "type": "basic"
        }
      ]
    }
  ]
}
